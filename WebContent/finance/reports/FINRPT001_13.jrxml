	<queryString>
		<![CDATA[select * from
(select xxxx.*,(qtd_rel_amnt+qtd_comm_contri-qtd_audit_amnt) as qtd_balance,
	     (ytd_rel_amnt+ytd_comm_contri-ytd_audit_amnt) as ytd_balance,
	     (ctd_rel_amnt+ctd_comm_contri-ctd_audit_amnt) as ctd_balance,
	     prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}') as qtr_end_date,
	     prwss_main.get_fin_year_code('$P!{fincode}') as finyear
from
(select divisional_name,sum(qtd_gpwsc) qtd_scheme,sum(ytd_gpwsc)ytd_scheme,sum(ctd_gpwsc)ctd_scheme,sum(qtd_expend) qtd_rel_amnt,sum(ytd_expend) ytd_rel_amnt,sum(ctd_expend) ctd_rel_amnt,
	sum(qtd_contri) qtd_comm_contri,sum(ytd_contri) ytd_comm_contri,sum(ctd_contri) ctd_comm_contri,sum(qtd_audited) qtd_audit_amnt,sum(ytd_audited) ytd_audit_amnt,sum(ctd_audited) ctd_audit_amnt
from
(select  ld.location_id,
        count(distinct case when loc_release_date between prwss_main.get_fin_qtr_start_date('$P!{fincode}'||'$P!{qtr}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}')
		 and loc_for='Installment-1' then ld.scheme_id else null end) as qtd_gpwsc,
	count(distinct case when loc_release_date between prwss_main.get_fin_year_start_date('$P!{fincode}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}')
		 and loc_for='Installment-1' then ld.scheme_id else null end) as ytd_gpwsc,
	count(distinct case when loc_release_date <= prwss_main.get_month_fin_year_end_date('$P!{month}','$P!{fincode}')
		 and loc_for='Installment-1' then ld.scheme_id else null end) as ctd_gpwsc,0 as qtd_expend,0 as ytd_expend,0 as ctd_expend,0 as qtd_contri,0 as ytd_contri,0 as ctd_contri,
		 0 as qtd_audited,0 as ytd_audited,0 as ctd_audited
from prwss_main.t_finance_loc_detail ld,prwss_main.t_finance_loc_header lh,prwss_main.mst_scheme_header sh
where lh.loc_id=ld.loc_id
and   ld.scheme_id=sh.scheme_id
group by ld.location_id

union all

-------------------------2 col--------------------------------
select vh.location_id,0 as qtd_gpwsc,0 as ytd_gpwsc,0 as ctd_gpwsc,
	sum(case when voc_date between prwss_main.get_fin_qtr_start_date('$P!{fincode}'||'$P!{qtr}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}') then vd.amount/100000 else 0 end) as qtd_expend,
        sum(case when voc_date between prwss_main.get_fin_year_start_date('$P!{fincode}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}') then vd.amount/100000 else 0 end) as ytd_expend,
	sum(case when voc_date <= prwss_main.get_month_fin_year_end_date('$P!{month}','$P!{fincode}')then vd.amount/100000 else 0 end) as ctd_expend,
	0 as qtd_contri,0 as ytd_contri,0 as ctd_contri,0 as qtd_audited,0 as ytd_audited,0 as ctd_audited
from prwss_main.t_finance_voucher_hdr vh,prwss_main.t_finance_voucher_detail vd
where vh.voc_id=vd.voc_id and vh.status<>'D' and vd.status<>'D'
and   vh.voc_type='PAYMENT' and component_id='com3'
group by vh.location_id

union all

------------------------3 col--------------------------------

select location_id,0 as qtd_gpwsc,0 as ytd_gpwsc,0 as ctd_gpwsc,0 as qtd_expend,0 as ytd_expend,0 as ctd_expend,
	sum(case when date_of_transaction between prwss_main.get_fin_qtr_start_date('$P!{fincode}'||'$P!{qtr}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}') then receipt_amount/100000 else 0 end) as qtd_contri,
        sum(case when date_of_transaction between prwss_main.get_fin_year_start_date('$P!{fincode}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}') then receipt_amount/100000 else 0 end) as ytd_contri,
	sum(case when date_of_transaction <= prwss_main.get_month_fin_year_end_date('$P!{month}','$P!{fincode}')then receipt_amount/100000 else 0 end) as ctd_contri,
	0 as qtd_audited,0 as ytd_audited,0 as ctd_audited
from prwss_main.t_finance_gpwsc_register
where status<>'D'
and   (receipt_type='Beneficiary-Share' or receipt_type like '%Gap%')
group by location_id

union all

---------------------------4 col--------------------------------
select ld.location_id,0 as qtd_gpwsc,0 as ytd_gpwsc,0 as ctd_gpwsc,0 as qtd_expend,0 as ytd_expend,0 as ctd_expend,0 as qtd_contri,0 as ytd_contri,0 as ctd_contri,
	sum(case when audit_completed_date between prwss_main.get_fin_qtr_start_date('$P!{fincode}'||'$P!{qtr}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}') then audited_amount/100000 else 0 end) as qtd_audited,
        sum(case when audit_completed_date between prwss_main.get_fin_year_start_date('$P!{fincode}') and  prwss_main.get_fin_qtr_end_date('$P!{fincode}'||'$P!{qtr}') then audited_amount/100000 else 0 end) as ytd_audited,
	sum(case when audit_completed_date <= prwss_main.get_month_fin_year_end_date('$P!{month}','$P!{fincode}')then audited_amount/100000 else 0 end) as ctd_audited
from prwss_main.t_finance_loc_detail ld,prwss_main.t_finance_loc_header lh
where lh.loc_id=ld.loc_id and ld.status<>'D' and lh.status<>'D'
group by ld.location_id) main,prwss_main.mst_divisional di
where main.location_id=di.divisional_id
group by divisional_name)xxxx where divisional_name like '%DPMC%' or divisional_name like '%SPMC%')main
where $P!{where}
order by divisional_name]]>
	</queryString>
