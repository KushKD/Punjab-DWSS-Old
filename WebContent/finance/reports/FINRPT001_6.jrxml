	<queryString>
		<![CDATA[select * from
(select * from
(select xy.*,amnt_transfer_to_gpwsc as amount_transfer_to_gpwsc,(release_amount-amnt_transfer_to_gpwsc) as amount_not_transfer_to_gpwsc
from
(select  zone_id,zone_name,circle_id,circle_name,district_id,district_name,request_to_location_id,divisional_id as location_id,divisional_name,
prwss_main.get_fin_year_code($P{fincode}) as fin_year,prwss_main.get_month_year($P{month},$P{fincode}) as mnth_yr,
scheme_id,sch_name,admin_amt,est_cost,
sum(case when installment_1 is not null and
     loc_release_date between prwss_main.get_fin_year_start_date($P{fincode}) and prwss_main.get_month_fin_year_end_date($P{month},$P{fincode})
     then installment_1 else 0 end) as installment_1,
sum(case when installment_2 is not null and
     loc_release_date between prwss_main.get_fin_year_start_date($P{fincode}) and prwss_main.get_month_fin_year_end_date($P{month},$P{fincode})
     then installment_2 else 0 end) as installment_2,
sum(case when installment_3 is not null and
     loc_release_date between prwss_main.get_fin_year_start_date($P{fincode}) and prwss_main.get_month_fin_year_end_date($P{month},$P{fincode})
     then installment_3 else 0  end) as installment_3,
sum(case when release_amount is not null and
     loc_release_date <= prwss_main.get_month_fin_year_end_date($P{month},$P{fincode})
     then release_amount else 0  end) as release_amount
from
(select z.zone_id,z.zone_name,c.circle_id,c.circle_name,md.district_id,md.district_name,mdi.divisional_id,mdi.divisional_name,
main.scheme_id,sch_name,(admin_app_gop_amount/100000)admin_amt,
	round((case when tech_app_ce_amount<>0 then tech_app_ce_amount
	else (case when tech_app_se_amount<>0 then tech_app_se_amount
	else tech_app_ee_amount end) end )/100000,2) as est_cost,loc_release_date,lh.request_to_location_id,
	case when loc_for='Installment-1' and release_amount is not null then (release_amount/100000) else 0 end as installment_1,
	case when loc_for='Installment-2' and release_amount is not null then (release_amount/100000) else 0 end as installment_2,
	case when loc_for='Installment-3' and release_amount is not null then (release_amount/100000) else 0 end as installment_3,
	case when release_amount is not null then (release_amount/100000) else 0 end as release_amount
from prwss_main.t_finance_loc_header lh,
(select ld.*,shd.admin_app_gop_amount,shd.tech_app_ce_amount,tech_app_ee_amount,tech_app_se_amount from prwss_main.t_finance_loc_detail ld
left join
(select sh.prog_id as program_id,sd.* from prwss_main.mst_scheme_header sh, prwss_main.mst_scheme_details sd
	where sh.scheme_id=sd.scheme_id
	and sh.prog_id='PROG15'
	and sh.status<>'D' and sd.status<>'D')shd
on ld.scheme_id=shd.scheme_id
where ld.status<>'D' and ld.program_id='PROG15') main,prwss_main.mst_divisional mdi,prwss_main.mst_district md,prwss_main.mst_zone z,prwss_main.mst_circle c
where z.zone_id=c.zone_id
and c.circle_id=md.circle_id
and lh.location_id=mdi.divisional_id
and   mdi.district_id=md.district_id
and lh.loc_id=main.loc_id
and lh.status<>'D'
) x
group by zone_id,zone_name,circle_id,circle_name,district_id,district_name,request_to_location_id,location_id,divisional_name,scheme_id,sch_name,admin_amt,est_cost)xy
left join
(select scheme_id,sum(case when receipt_type in ('Installment-1','Installment-2','Installment-3') then receipt_amount/100000 else 0 end) as amnt_transfer_to_gpwsc
from prwss_main.t_finance_gpwsc_register
where transaction_type='RECEIPT'
and date_of_transaction<=prwss_main.get_month_fin_year_end_date($P{month},$P{fincode})
group by scheme_id)xyz
on  xy.scheme_id=xyz.scheme_id) xxxx where (installment_1+installment_2+installment_3)>0 and amount_not_transfer_to_gpwsc>0)xyz
where $P!{where}
order by zone_name,circle_name,district_name,sch_name]]>
	</queryString>
