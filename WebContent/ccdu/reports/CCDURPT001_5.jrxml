	<queryString>
		<![CDATA[select * from
(select z.zone_id,zone_name as zone,block_name as block,c.circle_id,circle_name as circle,d.district_id,district_name as district,xx.*
from
(select x.*,item_name as material_name,quantity
from
(select location_id,ip.block_id,iec_progress_id,ip.village_id,village_name,iec_name as activity_name,activity_date,no_of_participants,outcome,date_part('month',activity_date) as activity_mnth
from prwss_main.t_ccdu_iec_progress ip,prwss_main.mst_village mv,prwss_main.mst_ccdu_iec_activity ia
where ip.village_id=mv.village_id
and   ip.activity_id=ia.iec_activity_id
and   mv.status<>'D' and ia.status<>'D'
and   ip.status <>'D'
)x

left join
(select mit.item_name,mu.quantity,mu.iec_progress_id
from
prwss_main.t_ccdu_iec_material_utilization mu,
prwss_main.mst_item mit
where mu.item_id=mit.item_id
and mu.status<>'D' and mit.status<>'D'
) mu_mit
on  x.iec_progress_id=mu_mit.iec_progress_id

) xx,prwss_main.mst_divisional di,prwss_main.mst_district d,prwss_main.mst_circle c,prwss_main.mst_zone z, prwss_main.mst_block mb
where xx.location_id=di.divisional_id
and   di.district_id=d.district_id
and   d.circle_id=c.circle_id
and   c.zone_id=z.zone_id
and   xx.block_id = mb.block_id
and   activity_date between to_date($P{from_date},'yyyy-mm-dd') and to_date($P{to_date},'yyyy-mm-dd')
) main
where $P!{where}
order by zone,circle,district,activity_name,activity_mnth]]>
	</queryString>
